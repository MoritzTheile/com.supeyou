<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	xmlns:m="urn:import:com.hotelorga.foundation.web.client.uiorga.flatbutton"
	>
	<ui:with field='text' type='com.hotelorga.foundation.web.client.resources.i18n.Text' />

	<g:FlowPanel styleName="detail-root"  ui:field="detailRoot">
	<g:FlowPanel styleName="actionbar"  ui:field="actionbar"/>
	<g:FlowPanel ui:field="formSlot"/>
	<g:FlowPanel styleName="buttons-slot"  ui:field="buttonsSlot">
		<m:FlatButtonWidget ui:field="cancelButton" text="{text.MULTIUSE_Cancel}"></m:FlatButtonWidget>
		<m:FlatButtonWidget ui:field="saveButton" text="{text.MULTIUSE_Save}"></m:FlatButtonWidget>
	</g:FlowPanel>
	</g:FlowPanel>	
</ui:UiBinder> 