<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client"
	>
	<ui:with field='text' type='com.hotelorga.foundation.web.client.resources.i18n.Text' />

	<g:FlowPanel  styleName="field-root" ui:field="fieldRoot">
		<g:Label  styleName="field-label" ui:field="fieldLabel"></g:Label>
		<g:FlowPanel  styleName="field-slot" ui:field="fieldSlot"></g:FlowPanel>
	</g:FlowPanel>

</ui:UiBinder> 